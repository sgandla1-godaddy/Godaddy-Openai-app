{
  "name": "godaddy-server",
  "version": "1.0.0",
  "description": "React Node.js server for GoDaddy application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "install:client": "cd client && npm install",
    "dev:client": "cd client && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:client\""
  },
  "keywords": [
    "react",
    "nodejs",
    "express",
    "godaddy"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "path": "^0.12.7",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}